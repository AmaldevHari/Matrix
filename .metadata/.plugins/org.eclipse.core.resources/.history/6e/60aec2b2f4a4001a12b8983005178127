#include "Matrix.h"


int main(void){


	float array1[9]= {1,1,3,1,1,7,9,4,8};


	int row1,col1;


	row1=3;col1=3;


	Matrix m(&array1[0], row1, col1);

	m.print_matrix();
	cout<<"\n";


	cout<<m.determinant()<<"\n";





	cout<<"inverse of this matrix is:"<<"\n";
	Matrix x;
	x= m.inverse();
	m.inverse().print_matrix();
	cout<<"\n";
	(m* m.inverse()).print_matrix();
	return 0;
}
